{
  "name": "generator-jhipster-azure-spring-apps",
  "version": "0.0.0",
  "private": true,
  "description": "Azure Spring Apps Application",
  "keywords": [
    "yeoman-generator",
    "jhipster-blueprint",
    "jhipster-7"
  ],
  "license": "MIT",
  "type": "module",
  "imports": {
    "#test-utils": "./test/utils.mjs"
  },
  "bin": {
    "jhipster-azure-spring-apps": "cli/cli.mjs"
  },
  "files": [
    "cli",
    "generators"
  ],
  "scripts": {
    "ejslint": "ejslint generators/**/*.ejs && ejslint generators/**/*.ejs -d '&'",
    "lint": "eslint .",
    "lint-fix": "npm run ejslint && npm run lint -- --fix",
    "mocha": "mocha generators --no-insight --forbid-only",
    "prettier-format": "prettier --write \"{,**/}*.{js,cjs,mjs,json,md,yml}\"",
    "prettier:check": "prettier --check \"{,src/**/,generators/**/}*.{md,json,yml,html,js,mjs,cjs,ts,tsx,css,scss,vue,java}\"",
    "prettier:format": "prettier --write \"{,src/**/,generators/**/}*.{md,json,yml,html,js,mjs,cjs,ts,tsx,css,scss,vue,java}\"",
    "pretest": "npm run lint",
    "test": "npm run mocha --",
    "update-snapshot": "npm run mocha -- --no-parallel --updateSnapshot"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "generator-jhipster": "7.9.3",
    "generator-jhipster-azure-spring-apps": "^1.0.1"
  },
  "devDependencies": {
    "ejs-lint": "2.0.0",
    "eslint": "8.43.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-mocha": "10.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "expect": "28.1.3",
    "mocha": "10.2.0",
    "mocha-expect-snapshot": "4.0.0",
    "prettier": "2.8.8",
    "prettier-plugin-java": "2.1.0",
    "prettier-plugin-packagejson": "2.2.18",
    "yeoman-test": "6.3.0"
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "cacheDirectories": [
    "node_modules"
  ]
}
